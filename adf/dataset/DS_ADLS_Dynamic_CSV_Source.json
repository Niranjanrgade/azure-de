{
	"name": "DS_ADLS_Dynamic_CSV_Source",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_ADLS",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "PlaceHolder"
			},
			"FilePath": {
				"type": "string",
				"defaultValue": "PlaceHolder"
			}
		},
		"folder": {
			"name": "Source"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().FilePath",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}